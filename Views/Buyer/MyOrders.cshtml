@model List<BookSwap.Models.Order>
@{
    ViewBag.Title = "My Orders";
}

<table class="table">
    <tr>
        <th>Book</th>
        <th>Quantity</th>
        <th>Total Price</th>
        <th>Status</th>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.bookid</td>
            <td>@order.quantity</td>
            <td>@order.totalprice</td>
            <td>@order.status</td>
        </tr>
    }
</table>
